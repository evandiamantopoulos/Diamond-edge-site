{
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)\\.(css|js|svg)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {
    "api/**.js": {
      "runtime": "nodejs20.x"
    }
  }
}